{
  "name": "memo-riz",
  "version": "1.0.0",
  "description": "Simple server for memo-riz",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\"  \"npm run watch-ts\" \"npm run watch-node\""
  },
  "author": "so-mouga",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/express-jwt": "0.0.42",
    "@types/node": "^11.13.6",
    "concurrently": "^4.1.0",
    "download": "^7.1.0",
    "https": "^1.0.0",
    "node-pre-gyp": "^0.12.0",
    "nodemon": "^1.18.11",
    "typescript": "^3.4.4"
  }
}
